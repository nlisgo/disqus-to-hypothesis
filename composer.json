{
    "require": {
        "elife/elife-ingest-xsl": "dev-disqus-to-hypothes-is",
        "elifesciences/elife-eif-schema": "dev-master",
        "league/html-to-markdown": "^4.5",
        "league/commonmark": "^0.15.6",
        "jasonrgd/disqus-php": "dev-master",
        "symfony/filesystem": "^3.3",
        "guzzlehttp/guzzle": "^6.3",
        "firebase/php-jwt": "^5.0"
    },
    "autoload": {
        "files": [
            "config.php",
            "functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\eLife\\": "./tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nlisgo/elife-ingest-xsl"
        },
        {
            "type": "package",
            "package": {
                "name": "elifesciences/elife-eif-schema",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/elifesciences/elife-eif-schema",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    }
}
